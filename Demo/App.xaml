<Application x:Class="Demo.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vm="clr-namespace:Demo.ViewModel"
             xmlns:con="clr-namespace:Demo.Converter"
             xmlns:local="clr-namespace:Demo"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
           
            <vm:ViewModelLocator x:Key="locator"/>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Dictionary/Share.xaml"/>
                <ResourceDictionary Source="/Dictionary/Style.xaml"/>
                <ResourceDictionary Source="/Dictionary/Template.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            
            
        </ResourceDictionary>
    </Application.Resources>
    
</Application>
